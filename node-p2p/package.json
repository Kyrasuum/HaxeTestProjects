{
    "name": "karalis",
    "bin": "bin/main.js",
    "version": "1.0.0",
    "description": "Simple 3D game test",
    "dependencies": {
        "hyperswarm": "^2.15.3",
        "pkg": "^5.5.2"
    },
    "pkg": {
        "targets": [
            "latest-linux-x64",
            "latest-windows-x64"
        ],
        "assets": [
            "node_modules/**/**/*"
        ],
        "scripts": "bin/**.js",
        "outputPath": "dist"
    },
    "scripts": {
        "build": "haxelib install Console.hx && haxe default.hxml && npm i",
        "dev": "npm run build && node bin/main.js",
        "dist": "npm run build && pkg .",
        "clean": "rm -rf node_modules package-lock.json bin dist"
    },
    "keywords": [],
    "author": "Kyrasuum",
    "license": "GNU"
}
